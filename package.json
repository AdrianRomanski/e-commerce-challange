{
  "name": "e-commerce-challange",
  "version": "0.0.1",
  "scripts": {
    "build": "ng build",
    "test": "ng test",
    "test:watch-false": "ng test --watch=false",
    "prettier:clean": "prettier . --write",
    "prettier:check": "prettier . --check",
    "start:dev": "ng serve --proxy-config src/config/proxy/proxy.dev.conf.json",
    "start:mock": "ng serve --proxy-config src/config/proxy/proxy.mock.conf.json",
    "start:json-server": "json-server --watch  mock/json-server/db.js",
    "start:mock-json-server": "concurrently \"yarn:start:mock\" \"yarn:start:json-server\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.0",
    "@angular/cdk": "^16.1.7",
    "@angular/common": "^16.1.0",
    "@angular/compiler": "^16.1.0",
    "@angular/core": "^16.1.0",
    "@angular/forms": "^16.1.0",
    "@angular/material": "^16.1.7",
    "@angular/platform-browser": "^16.1.0",
    "@angular/platform-browser-dynamic": "^16.1.0",
    "@angular/router": "^16.1.0",
    "bootstrap": "^5.3.1",
    "concurrently": "^8.2.0",
    "husky": "^8.0.3",
    "json-server": "^0.17.3",
    "prettier": "^3.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.0",
    "@angular/cli": "~16.1.0",
    "@angular/compiler-cli": "^16.1.0",
    "@types/jasmine": "~4.3.0",
    "eslint-plugin-prettier": "5.0.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.1.3",
    "husky": "^8.0.0"
  }
}
